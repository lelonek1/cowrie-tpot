[Unit]
Description=cowrie-telnet
Requires=docker.service
After=docker.service

[Service]
Restart=always
ExecStartPre=-/usr/bin/docker stop cowrie-telnet
ExecStartPre=-/usr/bin/docker rm -v cowrie-telnet
ExecStartPre=/bin/bash -c '/usr/bin/clean.sh cowrie-telnet off'
ExecStart=/usr/bin/docker run --name cowrie-telnet --rm=true -p 23:2223 -v /data/cowrie-telnet:/data/cowrie lelonek1/cowrie-tpot:latest1610
ExecStop=/usr/bin/docker stop cowrie-telnet

[Install]
WantedBy=multi-user.target
